{
  "name": "millerfabfieldinvoicerwebpack",
  "productName": "millerfabfieldinvoicerwebpack",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "Max",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/miller-fab-icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "millerfabfieldinvoicerwebpack"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "update-electron-app": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@electron-forge/cli": "6.0.0-beta.44",
    "@electron-forge/maker-deb": "6.0.0-beta.44",
    "@electron-forge/maker-rpm": "6.0.0-beta.44",
    "@electron-forge/maker-squirrel": "6.0.0-beta.44",
    "@electron-forge/maker-zip": "6.0.0-beta.44",
    "@electron-forge/plugin-webpack": "6.0.0-beta.44",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^3.2.0",
    "electron": "6.0.3",
    "electron-forge-maker-nsis": "^21.2.0",
    "exports-loader": "^0.7.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "webpack-dev-server": "^3.8.0"
  }
}
